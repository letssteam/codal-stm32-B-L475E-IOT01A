{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cortex-debug",
            "request": "launch",
            "servertype": "openocd",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/codal/build/DISCO_L475VG_IOT",
            "name": "Debug (OpenOCD)",
            "device": "STM32L475VGTx",
            "svdFile": "${workspaceFolder}/codal/libraries/codal-stm32-DISCO_L475VG_IOT/scripts/STM32L4x5.svd",
            "searchDir": [
                "/usr/share/openocd/scripts/",
                "${workspaceFolder}/codal/libraries/codal-stm32-DISCO_L475VG_IOT/scripts/"
            ],
            "configFiles": [
                "openocd.cfg"
            ],
            "preLaunchTask": "flash",
            "runToMain": true,
            "preLaunchCommands": [
                "set mem inaccessible-by-default off",
                "enable breakpoint",
                "monitor reset"
            ]
        }
    ]
}
